<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  
  <groupId>EastNets_Test_Automation</groupId>
  <artifactId>AML_Test_Automation</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>jar</packaging>
  
  <name>AML_Test_Automation</name>
  <url>http://maven.apache.org</url>
  
   <properties>
        <!-- specify which version of Java to build against-->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
         <aspectj.version>1.9.20</aspectj.version>
    </properties>
    <build>
    <plugins>
		<plugin>
		<groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.11.0</version>
		<configuration>
			<source>18</source>
			<target>18</target>
		</configuration>
		</plugin>
        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-surefire-plugin</artifactId>
            <version>3.0.0</version>
            <configuration>
                <suiteXmlFiles>
<!--					<suiteXmlFile>CustomerCardTestng.xml</suiteXmlFile>-->
<!--					<suiteXmlFile>CustomerDetailsTesting_Entity.xml</suiteXmlFile>-->
<!--					<suiteXmlFile>CustomerDetailsTesting_Finanical.xml</suiteXmlFile>-->
<!--					<suiteXmlFile>CustomerDetailsTesting_Individual.xml</suiteXmlFile>-->
					<suiteXmlFile>DetectionManagerTestng.xml</suiteXmlFile>
				</suiteXmlFiles>
				 <argLine>
                    -javaagent:"${settings.localRepository}/org/aspectj/aspectjweaver/${aspectj.version}/aspectjweaver-${aspectj.version}.jar"
                </argLine>

            </configuration>
            <dependencies>
                <dependency>
                    <groupId>org.aspectj</groupId>
                    <artifactId>aspectjweaver</artifactId>
                    <version>${aspectj.version}</version>
                </dependency>
            </dependencies>
        </plugin>
        <plugin>
                    <groupId>io.qameta.allure</groupId>
                    <artifactId>allure-maven</artifactId>
                    <version>2.10.0</version>
                    <configuration>
                        <reportVersion>2.14.0</reportVersion>
                    </configuration>
    </plugin>
    <!-- https://mvnrepository.com/artifact/io.qameta.allure/allure-maven -->
    </plugins>
</build>
